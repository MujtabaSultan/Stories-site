<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <%- include('../../partials/navbar.ejs') %>
    <h1>All stories</h1>

    <a href="/users/<%= user._id %>/crud/new">Add a new story</a>

    <ul>
      <%users.forEach(user =>{%> <%user.stories.forEach(story =>{%>

      <li>
        <a href="/users/<%=user._id%>/crud/<%= story.id %>"><%=story.name%></a>
      </li>

      <%})%> <%})%>
    </ul>
  </body>
</html>
